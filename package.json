{
  "name": "genai-fs",
  "version": "1.0.6",
  "description": "A terminal-based UI tool for managing Google Gemini FileStore.",
  "type": "module",
  "module": "src/index.tsx",
  "bin": {
    "genai-fs": "dist/index.js"
  },
  "scripts": {
    "dev": "bun run src/index.tsx",
    "start": "bun run src/index.tsx",
    "build": "bun build src/index.tsx --outdir dist --target node --format esm --external react-devtools-core"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "genai",
    "gemini",
    "filestore",
    "cli",
    "terminal",
    "ink",
    "react"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bellx2/genai-fs"
  },
  "dependencies": {
    "@google/genai": "^1.34.0",
    "@inkjs/ui": "^2.0.0",
    "ink": "^6.6.0",
    "react": "^19.2.3"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.2.7"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
